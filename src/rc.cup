                    {:
                        RESULT = ((MyParser) parser).DoBinaryExpr (_1, new OrOp(), _3);
                    :}
    |               Expr1:_1
                    {:
                        RESULT = _1;
                    :}
    ;


Expr1 ::=           Expr1:_1 T_AND Expr2:_3
                    {:
                        RESULT = ((MyParser) parser).DoBinaryExpr (_1, new AndOp(), _3);
                    :}
