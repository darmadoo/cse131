OptPointerList ::=  PointerList
    |               /* empty */
    ;

PointerList ::=     Pointer
    |               PointerList Pointer
    ;


Pointer ::=         T_STAR
    ;


OptArrayList ::=    ArrayList:_1
                    {:
                        RESULT = _1;
                    :}
    |               /* empty */
    ;

ArrayList ::=       Array:_1
                    {:
