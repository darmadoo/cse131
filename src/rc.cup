                    {:
                        RESULT = new ConstSTO (_1, new IntType(), Integer.parseInt(_1, 10));    // fix me!
                    :}
    |               T_FLOAT_LITERAL:_1
                    {:
                        RESULT = new ConstSTO (_1, new FloatType(), Float.parseFloat(_1));  // fix me!
                    :}
    |               T_STR_LITERAL:_1
                    {:
                        RESULT = new ConstSTO (_1);
                    :}
    |               T_TRUE
                    {:
                        RESULT = new ConstSTO ("true", new BoolType(), 1);    // fix me!
                    :}
    |               T_FALSE
                    {:
                        RESULT = new ConstSTO ("false", new BoolType(), 0);   // fix me!
                    :}
    |               T_NULLPTR
                    {:
                        RESULT = new ExprSTO ("nullptr", null);     // fix me!
                    :}
    |               T_THIS
                    {:
